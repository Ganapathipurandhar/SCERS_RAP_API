    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Data\Template.html" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Data\Template.html">
